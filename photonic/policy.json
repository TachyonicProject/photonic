{
    "role:root": "'Root' in req.context.roles",
    "role:ops": "'Operations' in req.context.roles",
    "role:admin": "'Administrator' in req.context.roles",
    "role:support": "'Support' in req.context.roles",
    "role:accounts": "'Account Manager' in req.context.roles",
    "role:billing": "'Billing' in req.context.roles",
    "role:customer": "'Customer' in req.context.roles",
    "system:login": "req.context.login is True",
    "system:admin": "role:root or role:ops",
    "users:admin": "role:root or role:ops or role:admin or role:support",
    "users:view": "users:admin",
    "roles:admin": "role:root",
    "roles:view": "roles:admin",
    "domains:admin": "role:root",
    "domains:view": "domains:admin",
    "tenants:admin": "role:root or role:ops or role:admin or role:support or role:billing or role:accounts or role:billing",
    "tenants:view": "tenants:admin",
    "services:admin": "system:login",
    "services:view": "system:login",
    "billing:admin": "role:root or role:billing",
    "billing:users": "system:login"
}
